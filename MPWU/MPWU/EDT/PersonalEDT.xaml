<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="MPWU.PersonalEDT">
	<ContentPage.Content>
		<Grid Margin="15">
			<Grid.RowDefinitions>
				<RowDefinition Height="30" />
				<RowDefinition Height="30" />
				<RowDefinition Height="30" />
				<RowDefinition Height="30" />
				<RowDefinition Height="30" />
			</Grid.RowDefinitions>
			<Grid.ColumnDefinitions>
				<ColumnDefinition Width="*" />
				<ColumnDefinition Width="*" />
			</Grid.ColumnDefinitions>
			<Label Text="Top Left" Grid.Row="0" Grid.Column="0" />
			<Entry Text="Top Right" Grid.Row="0" Grid.Column="1" />
			<Label Text="Bottom Left" Grid.Row="1" Grid.Column="0" />
			<Entry Text="Bottom Right" Grid.Row="1" Grid.Column="1" />
			<Label Text="Top Left" Grid.Row="2" Grid.Column="0" />
			<Entry Text="Top Right" Grid.Row="2" Grid.Column="1" />
			<Label Text="Bottom Left" Grid.Row="3" Grid.Column="0" />
			<Entry Text="Bottom Right" Grid.Row="3" Grid.Column="1" />
			<Label Text="Bottom Left" Grid.Row="4" Grid.Column="0" />
			<Entry Text="Bottom Right" Grid.Row="4" Grid.Column="1" />	
			<Label Text="Bottom Left" Grid.Row="4" Grid.Column="0" />
			<Entry Text="Bottom Right" Grid.Row="4" Grid.Column="1" />
		</Grid>
	</ContentPage.Content>
</ContentPage>
